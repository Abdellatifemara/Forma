{
  "fitness_for_busy_people": {
    "title": {
      "en": "Fitness for Busy People Guide",
      "ar": "دليل اللياقة للناس المشغولة"
    },
    "introduction": {
      "en": "You don't need hours at the gym to get results. This guide shows you how to maximize fitness with limited time. Consistency beats perfection.",
      "ar": "مش محتاج ساعات في الجيم عشان تحقق نتايج. الدليل ده بيوريك إزاي تعظّم اللياقة بوقت محدود. الاستمرارية بتغلب الكمال."
    },
    "mindset_shift": {
      "title": {
        "en": "The Busy Person's Mindset",
        "ar": "عقلية الشخص المشغول"
      },
      "principles": [
        {
          "principle": {
            "en": "Something is Always Better Than Nothing",
            "ar": "أي حاجة دايماً أحسن من لا حاجة"
          },
          "explanation": {
            "en": "A 15-minute workout beats no workout. Don't skip because you can't do a full session.",
            "ar": "تمرين 15 دقيقة أحسن من مفيش تمرين. ماتسقطش لأنك مش قادر تعمل جلسة كاملة."
          }
        },
        {
          "principle": {
            "en": "Consistency Over Intensity",
            "ar": "الاستمرارية فوق الشدة"
          },
          "explanation": {
            "en": "Three 20-minute sessions per week consistently will beat occasional 2-hour sessions.",
            "ar": "3 جلسات 20 دقيقة في الأسبوع بانتظام هتغلب جلسات ساعتين من وقت للتاني."
          }
        },
        {
          "principle": {
            "en": "Focus on the 20% That Gets 80% of Results",
            "ar": "ركز على الـ 20% اللي بتجيب 80% من النتايج"
          },
          "explanation": {
            "en": "Compound exercises, adequate protein, and sleep are more important than fancy workout programs.",
            "ar": "التمارين المركبة والبروتين الكافي والنوم أهم من برامج التمارين الفانسي."
          }
        },
        {
          "principle": {
            "en": "Integrate, Don't Add",
            "ar": "ادمج، ماتضفش"
          },
          "explanation": {
            "en": "Look for ways to add movement to existing routines rather than finding extra time.",
            "ar": "دور على طرق تضيف حركة لروتينك الحالي بدل ما تدور على وقت زيادة."
          }
        }
      ]
    },
    "quick_workout_strategies": {
      "title": {
        "en": "Quick Workout Strategies",
        "ar": "استراتيجيات التمرين السريع"
      },
      "strategies": [
        {
          "strategy": {
            "en": "Supersets",
            "ar": "السوبر سيتس"
          },
          "description": {
            "en": "Perform two exercises back-to-back with no rest, then rest. Cuts workout time nearly in half.",
            "ar": "اعمل تمرينين ورا بعض من غير راحة، بعدين ارتاح. بيقطع وقت التمرين تقريباً للنص."
          },
          "types": [
            {
              "type": {
                "en": "Antagonist Supersets (best for strength)",
                "ar": "سوبر سيتس متضادة (أفضل للقوة)"
              },
              "description": {
                "en": "Pair opposing muscle groups",
                "ar": "اجمع مجموعات عضلية متضادة"
              },
              "examples": [
                {
                  "en": "Bench Press + Barbell Row",
                  "ar": "بنش بريس + تجديف بار"
                },
                {
                  "en": "Bicep Curl + Tricep Pushdown",
                  "ar": "كيرل باي + بوش داون تراي"
                },
                {
                  "en": "Leg Extension + Leg Curl",
                  "ar": "ليج إكستنشن + ليج كيرل"
                }
              ]
            },
            {
              "type": {
                "en": "Same Muscle Supersets (more fatiguing)",
                "ar": "سوبر سيتس نفس العضلة (أكتر إرهاقاً)"
              },
              "description": {
                "en": "Hit the same muscle twice",
                "ar": "اضرب نفس العضلة مرتين"
              },
              "examples": [
                {
                  "en": "Bench Press + Push-ups",
                  "ar": "بنش بريس + بوش أبس"
                },
                {
                  "en": "Lat Pulldown + Dumbbell Row",
                  "ar": "سحب علوي + تجديف دمبل"
                }
              ]
            },
            {
              "type": {
                "en": "Upper/Lower Supersets",
                "ar": "سوبر سيتس علوي/سفلي"
              },
              "description": {
                "en": "Pair upper and lower body exercises",
                "ar": "اجمع تمارين علوي وسفلي"
              },
              "examples": [
                {
                  "en": "Overhead Press + Lunges",
                  "ar": "ضغط علوي + لانجز"
                },
                {
                  "en": "Pull-ups + Squats",
                  "ar": "عقلة + سكوات"
                }
              ]
            }
          ]
        },
        {
          "strategy": {
            "en": "Circuits",
            "ar": "الدوائر (سيركتس)"
          },
          "description": {
            "en": "Perform 4-6 exercises in a row with minimal rest, then rest and repeat. Great for fat loss and conditioning.",
            "ar": "اعمل 4-6 تمارين ورا بعض براحة قليلة، بعدين ارتاح وكرر. ممتاز لخسارة الدهون والكنديشننج."
          },
          "example_circuit": {
            "title": {
              "en": "15-Minute Full Body Circuit",
              "ar": "سيركت جسم كامل 15 دقيقة"
            },
            "instructions": {
              "en": "Perform each exercise for 40 seconds, rest 20 seconds, move to next. Complete 3 rounds.",
              "ar": "اعمل كل تمرين 40 ثانية، ارتاح 20 ثانية، انتقل للتالي. كمّل 3 جولات."
            },
            "exercises": [
              {
                "en": "Goblet Squat",
                "ar": "جوبلت سكوات"
              },
              {
                "en": "Push-ups",
                "ar": "بوش أبس"
              },
              {
                "en": "Dumbbell Row",
                "ar": "تجديف دمبل"
              },
              {
                "en": "Reverse Lunges",
                "ar": "لانجز خلفي"
              },
              {
                "en": "Plank",
                "ar": "بلانك"
              }
            ]
          }
        },
        {
          "strategy": {
            "en": "EMOM (Every Minute On the Minute)",
            "ar": "EMOM (كل دقيقة)"
          },
          "description": {
            "en": "Start each exercise at the top of every minute. Whatever time is left before the next minute is your rest.",
            "ar": "ابدأ كل تمرين في بداية كل دقيقة. الوقت المتبقي قبل الدقيقة الجاية هو راحتك."
          },
          "example": {
            "title": {
              "en": "12-Minute EMOM",
              "ar": "EMOM 12 دقيقة"
            },
            "exercises": [
              {
                "minute": "1, 4, 7, 10",
                "exercise": {
                  "en": "10 Squats",
                  "ar": "10 سكوات"
                }
              },
              {
                "minute": "2, 5, 8, 11",
                "exercise": {
                  "en": "10 Push-ups",
                  "ar": "10 بوش أبس"
                }
              },
              {
                "minute": "3, 6, 9, 12",
                "exercise": {
                  "en": "10 Rows",
                  "ar": "10 تجديف"
                }
              }
            ]
          }
        },
        {
          "strategy": {
            "en": "Compound-Only Workouts",
            "ar": "تمارين مركبة فقط"
          },
          "description": {
            "en": "Skip isolation exercises and focus only on big compound movements that work multiple muscles at once.",
            "ar": "تخطى تمارين العزل وركز بس على الحركات المركبة الكبيرة اللي بتشغل عضلات متعددة مرة واحدة."
          },
          "example": {
            "title": {
              "en": "20-Minute Compound Workout",
              "ar": "تمرين مركب 20 دقيقة"
            },
            "exercises": [
              {
                "exercise": {
                  "en": "Squat",
                  "ar": "سكوات"
                },
                "sets": "3",
                "reps": "8-10"
              },
              {
                "exercise": {
                  "en": "Bench Press or Push-ups",
                  "ar": "بنش بريس أو بوش أبس"
                },
                "sets": "3",
                "reps": "8-10"
              },
              {
                "exercise": {
                  "en": "Row",
                  "ar": "تجديف"
                },
                "sets": "3",
                "reps": "8-10"
              }
            ],
            "note": {
              "en": "Superset bench and rows to save time",
              "ar": "اعمل سوبر سيت البنش والتجديف لتوفير الوقت"
            }
          }
        },
        {
          "strategy": {
            "en": "Density Training",
            "ar": "تدريب الكثافة"
          },
          "description": {
            "en": "Do as many reps as possible of a few exercises in a set time. Track and try to beat it next time.",
            "ar": "اعمل أكبر عدد ممكن من الريبس لبعض التمارين في وقت محدد. تابع وحاول تغلبه المرة الجاية."
          },
          "example": {
            "en": "Set timer for 15 minutes. Do 5 pull-ups, 10 push-ups, 15 squats. Repeat as many rounds as possible.",
            "ar": "حط تايمر 15 دقيقة. اعمل 5 عقلة، 10 بوش أبس، 15 سكوات. كرر أكبر عدد ممكن من الجولات."
          }
        }
      ]
    },
