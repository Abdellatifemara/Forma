    "minimum_effective_dose": {
      "title": {
        "en": "The Minimum Effective Dose",
        "ar": "الجرعة الفعالة الأدنى"
      },
      "description": {
        "en": "What's the absolute minimum you can do and still see results? Here's the science-based answer:",
        "ar": "إيه الحد الأدنى المطلق اللي ممكن تعمله وبرضو تشوف نتايج؟ هنا الإجابة المبنية على العلم:"
      },
      "minimums": {
        "strength_maintenance": {
          "title": {
            "en": "To Maintain Strength",
            "ar": "للحفاظ على القوة"
          },
          "requirement": {
            "en": "1 session per week per muscle group, at high intensity",
            "ar": "جلسة واحدة في الأسبوع لكل مجموعة عضلية، بشدة عالية"
          }
        },
        "strength_gains": {
          "title": {
            "en": "To Build Strength",
            "ar": "لبناء القوة"
          },
          "requirement": {
            "en": "2-3 sessions per week per muscle group",
            "ar": "2-3 جلسات في الأسبوع لكل مجموعة عضلية"
          }
        },
        "muscle_maintenance": {
          "title": {
            "en": "To Maintain Muscle",
            "ar": "للحفاظ على العضل"
          },
          "requirement": {
            "en": "1/3 to 1/2 of what you did to build it (about 6-9 sets per muscle per week)",
            "ar": "1/3 إلى 1/2 من اللي عملته عشان تبنيه (حوالي 6-9 سيتات لكل عضلة في الأسبوع)"
          }
        },
        "muscle_gains": {
          "title": {
            "en": "To Build Muscle",
            "ar": "لبناء العضل"
          },
          "requirement": {
            "en": "10-20 working sets per muscle per week",
            "ar": "10-20 سيت شغّال لكل عضلة في الأسبوع"
          }
        },
        "cardio_health": {
          "title": {
            "en": "For Heart Health",
            "ar": "لصحة القلب"
          },
          "requirement": {
            "en": "150 minutes moderate activity OR 75 minutes vigorous activity per week (WHO guidelines)",
            "ar": "150 دقيقة نشاط متوسط أو 75 دقيقة نشاط شديد في الأسبوع (توصيات WHO)"
          }
        }
      },
      "practical_minimum": {
        "title": {
          "en": "Practical Minimum Program",
          "ar": "البرنامج العملي الأدنى"
        },
        "program": {
          "en": "2 full-body sessions per week, 20-30 minutes each, focusing on compound exercises",
          "ar": "جلستين فول بادي في الأسبوع، 20-30 دقيقة لكل واحدة، بالتركيز على التمارين المركبة"
        },
        "example": {
          "session": [
            {
              "exercise": {
                "en": "Squat or Leg Press",
                "ar": "سكوات أو ليج بريس"
              },
              "sets": "2-3",
              "reps": "8-12"
            },
            {
              "exercise": {
                "en": "Push (Bench, Push-ups, or Machine)",
                "ar": "دفع (بنش، بوش أبس، أو جهاز)"
              },
              "sets": "2-3",
              "reps": "8-12"
            },
            {
              "exercise": {
                "en": "Pull (Row or Pulldown)",
                "ar": "سحب (تجديف أو سحب)"
              },
              "sets": "2-3",
              "reps": "8-12"
            },
            {
              "exercise": {
                "en": "Hinge (RDL or Hip Thrust)",
                "ar": "هينج (RDL أو هيب ثرست)"
              },
              "sets": "2-3",
              "reps": "8-12"
            }
          ]
        }
      }
    },
