  "choosingGuide": {
    "factors": {
      "en": [
        {
          "factor": "Location",
          "importance": "high",
          "tip": "Choose a gym close to home or work - you're more likely to go consistently"
        },
        {
          "factor": "Equipment",
          "importance": "high",
          "tip": "Visit during peak hours to see if it's too crowded for your needs"
        },
        {
          "factor": "Hours",
          "importance": "medium",
          "tip": "Make sure the gym is open when you can train"
        },
        {
          "factor": "Price",
          "importance": "medium",
          "tip": "Consider annual memberships for savings, but try monthly first"
        },
        {
          "factor": "Amenities",
          "importance": "low",
          "tip": "Focus on what you'll actually use - pools and saunas are nice but not essential"
        }
      ],
      "ar": [
        {
          "factor": "الموقع",
          "importance": "عالي",
          "tip": "اختار جيم قريب من بيتك أو شغلك - هتروح بانتظام أكتر"
        },
        {
          "factor": "المعدات",
          "importance": "عالي",
          "tip": "زور الجيم في أوقات الذروة عشان تشوف لو مزدحم أوي لاحتياجاتك"
        },
        {
          "factor": "المواعيد",
          "importance": "متوسط",
          "tip": "اتأكد إن الجيم مفتوح في الوقت اللي تقدر تتدرب فيه"
        },
        {
          "factor": "السعر",
          "importance": "متوسط",
          "tip": "فكر في الاشتراكات السنوية للتوفير، بس جرب الشهري الأول"
        },
        {
          "factor": "المرافق",
          "importance": "منخفض",
          "tip": "ركز على اللي هتستخدمه فعلاً - الحمامات والساونا حلوة بس مش أساسية"
        }
      ]
    },
