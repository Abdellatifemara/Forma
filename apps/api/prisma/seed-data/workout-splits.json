  "workoutSplits": [
    {
      "id": "full-body",
      "name": {
        "en": "Full Body Split",
        "ar": "تمرين الجسم الكامل"
      },
      "description": {
        "en": "Train your entire body in each workout session, typically 3 times per week with rest days in between.",
        "ar": "تمرن جسمك كله في كل جلسة تدريب، عادة 3 مرات في الأسبوع مع أيام راحة بينهم."
      },
      "frequency": "3 days/week",
      "bestFor": {
        "en": ["Beginners", "Those with limited time", "General fitness", "Strength building"],
        "ar": ["المبتدئين", "اللي وقتهم محدود", "اللياقة العامة", "بناء القوة"]
      },
      "pros": {
        "en": [
          "Hit each muscle 3x per week for optimal growth",
          "Flexible scheduling - miss a day, still trained everything",
          "Great for beginners to learn movements",
          "Burns more calories per session",
          "Better recovery between sessions"
        ],
        "ar": [
          "بتمرن كل عضلة 3 مرات في الأسبوع للنمو الأمثل",
          "جدول مرن - لو فاتك يوم، برضو مرنت كل حاجة",
          "ممتاز للمبتدئين يتعلموا الحركات",
          "بتحرق سعرات أكتر في كل جلسة",
          "استشفاء أحسن بين الجلسات"
        ]
      },
      "cons": {
        "en": [
          "Sessions can be long (60-90 minutes)",
          "Hard to focus on weak points",
          "Can be tiring by end of workout",
          "Less volume per muscle per session"
        ],
        "ar": [
          "الجلسات ممكن تكون طويلة (60-90 دقيقة)",
          "صعب تركز على النقط الضعيفة",
          "ممكن تتعب آخر التمرين",
          "فوليوم أقل لكل عضلة في الجلسة"
        ]
      },
      "sampleSchedule": {
        "en": {
          "Monday": "Full Body A",
          "Tuesday": "Rest",
          "Wednesday": "Full Body B",
          "Thursday": "Rest",
          "Friday": "Full Body C",
          "Saturday": "Rest/Cardio",
          "Sunday": "Rest"
        },
        "ar": {
          "الاثنين": "جسم كامل أ",
          "الثلاثاء": "راحة",
          "الأربعاء": "جسم كامل ب",
          "الخميس": "راحة",
          "الجمعة": "جسم كامل ج",
          "السبت": "راحة/كارديو",
          "الأحد": "راحة"
        }
      },
