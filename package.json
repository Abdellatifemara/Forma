{
  "name": "forma",
  "version": "1.0.0",
  "description": "Forma - Shape Your Future - Egyptian Fitness App",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev:web",
    "dev:api": "npm run dev --workspace=apps/api",
    "dev:mobile": "npm run start --workspace=apps/mobile",
    "dev:web": "npm run dev --workspace=apps/web",
    "build": "npm run build:packages && npm run build:apps",
    "build:packages": "npm run build --workspace=packages/types && npm run build --workspace=packages/shared",
    "build:apps": "npm run build:api && npm run build:web",
    "build:api": "npm run build --workspace=apps/api",
    "build:web": "npm run build --workspace=apps/web",
    "test": "npm run test --workspaces --if-present",
    "lint": "npm run lint --workspaces --if-present",
    "type-check": "npm run type-check --workspaces --if-present",
    "clean": "npm run clean --workspaces --if-present",
    "db:generate": "npm run db:generate --workspace=apps/api",
    "db:migrate": "npm run db:migrate --workspace=apps/api",
    "db:seed": "npm run db:seed --workspace=apps/api",
    "db:studio": "npm run db:studio --workspace=apps/api"
  },
  "keywords": [
    "fitness",
    "egypt",
    "workout",
    "nutrition",
    "arabic"
  ],
  "author": "Forma Team",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=18.0.0"
  },
  "overrides": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}
